{
    "name": "Yosemite Lofi Image Detection",
    "author": "Andrei F",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        },
        "default_title": "Yosemite Lofi Image Detection"
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-scripts/extension.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}
